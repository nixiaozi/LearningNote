<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1</TargetFrameworks>
    <Authors>Ivan Mihaylovskyy, Jim Evans</Authors>
    <Description>Extensible HTTP proxy with SSL support</Description>
    <Copyright>Copyright © 2019 Ivan Mihaylovskyy, Jim Evans</Copyright>
    <PackageProjectUrl>http://github.com/jimevans/BenderProxy</PackageProjectUrl>
    <Version>1.1.2</Version>
    <PackageTags>HTTP Proxy</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>1.1.2.0</AssemblyVersion>
    <FileVersion>1.1.2.0</FileVersion>
    <PackageReleaseNotes>See release notes at https://github.com/jimevans/BenderProxy/blob/master/RELEASE_NOTES</PackageReleaseNotes>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="BenderProxy.nuspec" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="src\" />
  </ItemGroup>

</Project>
